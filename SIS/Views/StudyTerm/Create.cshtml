@model SIS.Models.StudyTerm

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Create</title>
</head>
<body>

<h4>StudyTerm</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="TermId" class="control-label"></label>
                <input asp-for="TermId" class="form-control" />
                <span asp-validation-for="TermId" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="TermName" class="control-label"></label>
                <input asp-for="TermName" class="form-control" />
                <span asp-validation-for="TermName" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="TermStartDate" class="control-label"></label>
                <input asp-for="TermStartDate" class="form-control" />
                <span asp-validation-for="TermStartDate" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="TermEndDate" class="control-label"></label>
                <input asp-for="TermEndDate" class="form-control" />
                <span asp-validation-for="TermEndDate" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="TermYear" class="control-label"></label>
                <input asp-for="TermYear" class="form-control" />
                <span asp-validation-for="TermYear" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="TermSeason" class="control-label"></label>
                <input asp-for="TermSeason" class="form-control" />
                <span asp-validation-for="TermSeason" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Create" class="btn btn-default" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">Back to List</a>
</div>

</body>
</html>
